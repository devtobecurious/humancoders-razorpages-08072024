@page
@* @using SdA.AFrodoJourney.Core.Models *@

@model PeopleModel

@{
	//Layout = "_LayoutSansBootstrap";

	var list = new List<Person>()
	{
		new Person() { Id = 1, Surname = "Frodon" },
		new () { Id = 2, Surname = "Sam" },
		new () { Id = 3, Surname = "Gimli" },
	};
}

<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Prénom</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in list)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.Surname</td>
			</tr>
		}
		
	</tbody>
</table>

